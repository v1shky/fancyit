{
  "name": "v1shky/fancyit",
  "description": "A package to generate fancy string from normal string",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "V1shky\\Fancyit\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "V1shky\\Fancyit\\FancyitServiceProvider"
      ],
      "aliases": {
        "Fancyit": "V1shky\\Fancyit\\FancyitFacade"
      }
    }
  }
}
